{
  "name": "boilerplatevaq",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/utils": "^8.12.0",
    "@graphql-yoga/node": "^2.13.9",
    "@types/babel__template": "^7.4.1",
    "@types/node": "^18.7.13",
    "@types/uuid": "^8.3.4",
    "add": "^2.0.6",
    "babel-jest": "^29.0.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tools": "^8.3.6",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.7",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "nodemon --exec ts-node --files src/index.ts",
    "test-server": "NODE_ENV=test ts-node --files src/index.ts",
    "dev": "NODE_ENV=development ts-node --files ./src/index.ts",
    "vaq": "nodemon src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
    "test": "NODE_ENV=test jest"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.0.1",
    "@types/node": "^16.11.10",
    "gql2ts": "^1.10.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^29.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
